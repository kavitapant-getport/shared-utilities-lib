{
  "name": "@yourorg/shared-utilities-lib",
  "version": "2.1.4",
  "description": "Shared utilities and helper functions for microservices",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/walmart-demo-org/shared-utilities-lib.git"
  },
  "keywords": [
    "utilities",
    "helpers",
    "microservices",
    "validation",
    "api",
    "logging"
  ],
  "author": "Platform Team <platform@yourorg.com>",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "rollup -c",
    "build:watch": "rollup -c --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write src/**/*.ts",
    "prepublishOnly": "npm run build && npm test"
  },
  "dependencies": {
    "validator": "^13.9.0",
    "date-fns": "^2.30.0",
    "axios": "^1.4.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/jest": "^29.5.0",
    "typescript": "^5.0.0",
    "jest": "^29.5.0",
    "rollup": "^3.21.0",
    "eslint": "^8.40.0",
    "prettier": "^2.8.0"
  }
}
